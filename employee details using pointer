#include<iostream>
using namespace std;
class employee{
    public:
    string name;
    float salary;
    int id;
    void get(){
        cin>>name>>salary>>id;
    }
        void disp(){
            cout<<name<<endl;
            cout<<salary<<endl;
            cout<<id<<endl;
    }
};
int main(){
    employee emp;
    employee *ptr=&emp;
    ptr->get();
    ptr->disp();
}
