#include<iostream>
using namespace std;
int main(){
    int n,r,num,sum=0;
    cout<<"enter the number ";
    cin>>n;
    num=n;
    for(int i=1;i<=n/2;i++){
        if(n%i==0){
            sum=sum+i;
        }
    }
    if(sum>num){
        cout<<"abundant";
    }else{
        cout<<"not abundant";
    }
}
