#include<iostream>
using namespace std;
int array(int a[5]){
    cout<<"integer array ";
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
    cout<<endl;
    return 0;
}
double array(double b[5]){
    cout<<"double array ";
    for(int i=0;i<5;i++){
        cout<<b[i]<<" ";
    }
    cout<<endl;
    return 0;
}
char array(char c[5]){
    cout<<"character array ";
    for(int i=0;i<5;i++){
        cout<<c[i]<<" ";
    }
    cout<<endl;
    return 0;
}
int main(){
    int intarray[]={1,2,3,4,5};
    double doublearray[]={2.1,3.4,5.6,7.8,9.4};
    char chararray[]={'a','b','c','d','e'};
    cout<<array(intarray);
    cout<<array(doublearray);
    cout<<array(chararray);
}
