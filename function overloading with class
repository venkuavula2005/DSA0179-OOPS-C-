#include<iostream>
using namespace std;
class sample{
    private:
    int i;
    char c;
    double f;
    string s;
    public:
    void disp(int i){
        cout<<i<<endl;
    }
    void disp(char c){
        cout<<c<<endl;
    }
    void disp(double f){
        cout<<f<<endl;
    }
    void disp(string s){
        cout<<s<<endl;
    }
};
int main(){
    sample s;
    s.disp(3);
    s.disp('a');
    s.disp(3.4);
    s.disp("welcome");
}
