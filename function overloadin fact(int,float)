#include<iostream>
using namespace std;
int factorial(int n){
    int fact=1;
    for(int i=1;i<=n;i++){
        fact*=i;
        
    }
    return fact;
}
float factorial(float f){
    float fact=1;
    for(float i=1;i<=f;i++){
        fact=fact*i;
    }
    return fact;
}
int main(){
    int n=5;
    float f=2.2;
    cout<<factorial(n)<<endl;
    cout<<factorial(f)<<endl;
}
