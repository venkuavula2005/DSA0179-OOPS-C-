#include<iostream>
using namespace std;
int main(){
    int n,num,r,sum=0;
    cout<<"enter the number";
    cin>>n;
    num=n;
    int square=n*n;
    while(square>0){
        r=square%10;
        sum+=r;
        square/=10;
    }
    if(sum==num){
        cout<<"neon number";
    }else{
        cout<<"not neon number";
    }
}
