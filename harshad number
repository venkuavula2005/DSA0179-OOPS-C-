#include<iostream>
using namespace std;
int main(){
    int n,r,sum=0;
    cout<<"enter the number";
    cin>>n;
    int num=n;
    while(n>0){
        r=n%10;
        sum+=r;
        n/=10;
    }
    if(num%sum==0){
        cout<<"harshad number";
    }else{
        cout<<"not harshad number";
    }
    return 0;
}
