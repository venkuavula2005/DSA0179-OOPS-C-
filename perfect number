#include<iostream>
using namespace std;
int main(){
    int i,n,sum=0,r;
    cout<<"enter the number ";
    cin>>n;
    int num=n;
    for(i=1;i<n;i++){
        if(n%i==0){
            sum+=i;
        }
    }
    if(sum==num){
        cout<<"perfect number";
    }else{
        cout<<"not perfect number";
    }
}
