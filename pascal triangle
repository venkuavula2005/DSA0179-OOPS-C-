#include<iostream>
using namespace std;
int main(){
    int i,j,s,n,a;
    cout<<"enter the number";
    cin>>n;
    for(i=1;i<=n;i++){
        a=1;
        for(s=1;s<=n-i;s++){
            cout<<" ";
        }
        for(j=1;j<=i;j++){
            cout<<a<<" ";
            a=a*(i-j)/j;
        }
        cout<<" "<<endl;
    }
}
