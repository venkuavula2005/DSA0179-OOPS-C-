#include<iostream>
#include<cmath>
using namespace std;
int max(int a,int b){
    return (a>b)?a:b;
}
float max(float f1,float f2){
    return (f1>f2)?f1:f2;
}
char max(char c1,char c2){
    return (c1>c2)?c1:c2;
}
int main(){
    int a=10,b=20;
    float f1=4.5,f2=2.1;
    char c1='e',c2='r';
    cout<<max(a,b)<<endl;
    cout<<max(f1,f2)<<endl;
    cout<<max(c1,c2)<<endl;
}
